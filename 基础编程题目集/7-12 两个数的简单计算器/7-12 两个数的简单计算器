#include<iostream>
#include<cstdio>
using namespace std;
int main() {
	int x, y;
	char c1;
	cin >> x >> c1 >>  y;
	switch (c1)
	{
	case'+': 
		cout << x + y << endl;
		break;
	case'-':
		cout << x - y << endl;
		break;
	case'*':
		cout << x * y << endl;
		break;
	case'/':
		if (y == 0)break;
		cout << x / y << endl;
		break;
	case'%':
		if (y == 0)break;
		cout << x % y << endl;
		break;
	default:
		cout << "ERROR" << endl;
		break;
	}
	
	return 0;
}